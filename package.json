{
  "name": "hexo-auto-tagger",
  "description": "Automatically adds tags to posts if the post contains any predefined key tag word or phrases.",
  "version": "0.1.1",
  "main": "index",
  "scripts": {
    "eslint": "eslint .",
    "jscs": "jscs .",
    "lint": "jscs . && eslint .",
    "test": "mocha test/index.js",
    "test-cov": "istanbul cover --print both _mocha -- test/index.js",
    "prepush": "npm run lint && npm run test-cov"
  },
  "directories": {
    "lib": "./lib"
  },
  "repository": "hexojs/hexo-generator-tag",
  "keywords": [
    "hexo",
    "tag",
    "posts",
    "auto-tagger"
  ],
  "author": "Gregory Pike",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-hexo": "^1.0.2",
    "hexo": "^3.1.1",
    "istanbul": "^0.4.2",
    "jscs": "^2.9.0",
    "jscs-preset-hexo": "^1.0.1",
    "mocha": "^2.4.5"
  }
}
